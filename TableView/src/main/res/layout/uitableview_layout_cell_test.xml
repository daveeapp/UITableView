<?xml version="1.0" encoding="utf-8"?>
<app.davee.assistant.uitableview.UITableViewCell xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/UITableView.UITableViewCell.Default"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cellAccessoryType="Disclosure"
    app:cellImage="@drawable/ic_check_blue_24dp"
    app:cellSelectionStyle="Gray"
    app:cellStyle="Value1">
    
    <app.davee.assistant.uitableview.cell.ContentView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        
        <TextView
            android:id="@+id/custom_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_marginTop="10dp"
            android:text="Test Title" />
        
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@id/custom_title"
            android:layout_alignParentEnd="true"
            android:text="Test Value" />
        
        <TimePicker
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/custom_title"
            android:layout_marginTop="10dp"
            android:visibility="visible" />
    
    </app.davee.assistant.uitableview.cell.ContentView>
    
    <!--<ImageView-->
    <!--android:layout_width="match_parent"-->
